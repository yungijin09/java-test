import java.util.Scanner;

public class ForTest6 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("정수 10개를 입력하세요");
        int[] num = new int[10];
        int cnt = 0;
        for (int i = 0; i < 10; i++) {
            num[i] = sc.nextInt();
            if(num[i] % 3 == 0 || num[i] % 5 == 0){
                cnt++;
            }
        }
        System.out.println("3과 5의 배수 : " + cnt);
    }
}
